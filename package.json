{
  "name": "@alveo-vl/jsalveo",
  "version": "0.1.0",
  "description": "JavaScript (ES6) library for Alveo backend",
  "main": "dist/jsalveo.bundle.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run browserify && npm run browserify-core",
    "release": "npm publish",
    "browserify": "browserify src/index.js -o dist/jsalveo.bundle.js -t [ babelify --presets [ '@babel/preset-env' ] ]",
    "browserify-core": "browserify src/index.js -x dexie -x @babel/runtime -x swagger-client -o dist/jsalveo.core.bundle.js -t [ babelify --presets [ '@babel/preset-env' ] ]"
  },
  "author": "David L. Irving <david.l.irving@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alveo/jsalveo"
  },
  "babel": {
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.49",
    "dexie": "^2.0.3",
    "swagger-client": "^3.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "babel-loader": "^7.1.4",
    "babelify": "^9.0.0",
    "browserify": "^16.2.2"
  }
}
